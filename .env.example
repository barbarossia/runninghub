# RunningHub API Configuration
# Copy this file to .env and fill in your values

# Required: Your RunningHub API key
RUNNINGHUB_API_KEY=your_api_key_here

# Required: Your workflow ID for the AI application
RUNNINGHUB_WORKFLOW_ID=your_workflow_id_here

# Optional: API host (default: www.runninghub.cn)
RUNNINGHUB_API_HOST=www.runninghub.cn

# Optional: Download directory for output files
RUNNINGHUB_DOWNLOAD_DIR=~/Downloads

# Optional: Image folder for batch processing
RUNNINGHUB_IMAGE_FOLDER=.

# Optional: Folder name for extracted video frames (default: clipped_images)
RUNNINGHUB_CLIP_OUTPUT_FOLDER=clipped_images

# Optional: Node ID to human-readable alias mapping (JSON format)
RUNNINGHUB_NODE_MAPPING={"203":"image","231":"trigger word","235":"width","236":"height"}
